<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:title="@string/general">
        <ListPreference
            android:enabled="false"
            android:key="promotion"
            android:title="@string/promotions" />
        <CheckBoxPreference
            android:key="name_search"
            android:summary="@string/name_search_off"
            android:summaryOff="@string/name_search"
            android:title="@string/advanced_search" />
        <mines.ales.agenda.emagenda.view.AutoCompletePreference
            android:dependency="name_search"
            android:key="name"
            android:title="@string/name" />
    </PreferenceCategory>
</PreferenceScreen>